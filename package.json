{
  "name": "sozai",
  "version": "0.3.1",
  "description": "UI framework with material components build with Svelte and without Tailwind CSS",
  "svelte": "src/index.js",
  "scripts": {
    "build": "rollup -c",
    "build:examples": "cd examples && rollup -c",
    "dev": "cd examples && rollup -c -w",
    "start": "sirv examples --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.5.0",
    "sirv-cli": "^1.0.0"
  },
  "keywords": [
    "svelte", "material-design"
  ],
  "files": [
    "src",
    "index.js",
    "index.d.ts"
  ]
}
